<% @page_title = "Login" %>
<div class="Login_Form">
  <h1>Welcome back.  Please login.</h1>
  <%= form_for(:login, :url => {:controller => 'logins', :action => 'create'}) do |f| %>
      <p> Email:</br> <%= f.text_field :email%> </p>
      <p> Password:</br> <%= f.password_field :password%></p>
      <%= f.button :Login %>
  <% end %>
</div>
